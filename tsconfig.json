{
  "compilerOptions": {
    "strict": true,
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "vite/client",
      "vike-solid/client"
    ],
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "jsxImportSource": "solid-js",
    "paths": {
      "@/registry/*": ["./components/registry/*"],
      "@/*": ["./*"]
    }
  },
  "exclude": [
    "dist"
  ]
}
